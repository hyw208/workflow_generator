<?xml version="1.0" ?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" targetNamespace="http://bpmn.io/schema/bpmn" id="main_definitions">
  <process id="main_process" isExecutable="true" camunda:historyTimeToLive="180">
    <startEvent id="StartEvent"/>
    <serviceTask id="Task1" name="Call API As Test">
      <extensionElements>
        <camunda:connector>
          <camunda:connectorId>http-connector</camunda:connectorId>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://host.docker.internal:8081</camunda:inputParameter>
            <camunda:inputParameter name="method">POST</camunda:inputParameter>
            <camunda:inputParameter name="payload">{"name": "main", "z": 3}</camunda:inputParameter>
          </camunda:inputOutput>
        </camunda:connector>
      </extensionElements>
    </serviceTask>
    <userTask id="Task2" name="Human Review Indicator" camunda:formKey="x/y/z/main.html">
      <extensionElements>
        <camunda:meta key="x">1</camunda:meta>
        <camunda:meta key="y">2</camunda:meta>
        <camunda:meta key="z">3</camunda:meta>
      </extensionElements>
    </userTask>
    <callActivity id="CallRSAWorkflow" name="Call RSA Workflow" calledElement="rsa_process">
      <extensionElements>
        <camunda:in source="xxx" target="xxx"/>
        <camunda:out source="yyy" target="yyy"/>
      </extensionElements>
    </callActivity>
    <endEvent id="EndEvent"/>
    <sequenceFlow id="Flow_StartEvent_to_Task1" sourceRef="StartEvent" targetRef="Task1"/>
    <sequenceFlow id="Flow_Task1_to_Task2" sourceRef="Task1" targetRef="Task2"/>
    <sequenceFlow id="Flow_Task2_to_CallRSAWorkflow" sourceRef="Task2" targetRef="CallRSAWorkflow"/>
    <sequenceFlow id="Flow_CallRSAWorkflow_to_EndEvent" sourceRef="CallRSAWorkflow" targetRef="EndEvent"/>
  </process>
</definitions>
